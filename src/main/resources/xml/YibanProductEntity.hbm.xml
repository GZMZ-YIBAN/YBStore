<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.YibanProductEntity" table="yiban_product" schema="yiban">
        <id name="id">
            <column name="id" sql-type="int(3)"/>
        </id>
        <property name="name">
            <column name="`name`" sql-type="varchar(255)"/>
        </property>
        <property name="describe">
            <column name="`describe`" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="number">
            <column name="`number`" sql-type="int(5)"/>
        </property>
        <property name="price">
            <column name="`price`" sql-type="int(5)"/>
        </property>
        <property name="photo">
            <column name="`photo`" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="enable">
            <column name="`enable`" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>